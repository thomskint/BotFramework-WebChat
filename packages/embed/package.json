{
  "name": "embed",
  "version": "0.0.0-0",
  "description": "",
  "main": "src/index.js",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/*.{spec,test}.{js,jsx,ts,tsx}",
      "!**/__tests__/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "testEnvironment": "node"
  },
  "scripts": {
    "build": "npm run build:babel && npm run build:webpack",
    "build:babel": "babel src --ignore **/*.spec.js,**/*.test.js,__tests__/**/*.js --out-dir lib --verbose",
    "build:webpack": "webpack-cli",
    "prestart": "npm run build:babel",
    "start": "concurrently --kill-others --names \"babel,server,webpack\" \"npm run start:babel\" \"npm run start:server\" \"npm run start:webpack\"",
    "start:babel": "npm run build:babel -- --skip-initial-build --watch",
    "start:server": "node-dev --no-notify --respawn hostDevServer",
    "start:webpack": "npm run build:webpack -- --watch",
    "test": "jest --watch",
    "test:ci": "npm run test -- --ci --coverage true --no-watch"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.20.0",
    "jest": "^24.9.0",
    "node-dev": "^4.0.0",
    "node-fetch": "^2.6.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "restify": "^8.5.1",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "core-js": "^3.5.0",
    "url-search-params-polyfill": "^7.0.0",
    "whatwg-fetch": "^3.0.0"
  }
}
